/**app.wxss**/

form{
  width: 100%;
  height: 100%;
}
.wran-tips {
  text-align: center;
  color: #fff;
  height: 30px;
  line-height: 30px;
  /* padding: 2%; */
  width: 100%;
  background-color: #f00;
  display: flex;
  justify-content: center;
  position: fixed;
  top: 0;
  left: 0;
  border-radius: 5px;
  font-size: 30rpx;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 20rpx 5px;
  box-sizing: border-box;
  z-index: 0;
}

.b1 {
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0px;
  background: rgba(0, 0, 0, 0.7);
  overflow: hidden;
  z-index: -1;
}